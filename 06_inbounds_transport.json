{
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "tag": "sk5",
            "port": S5_PORT,
            "protocol": "socks",
            "settings": {
                "auth": "password",
                "udp": true,
                "accounts": [
                    {
                        "user": "S5_USER",
                        "pass": "S5_PASS"
                    }
                ]
            },
            "streamSettings": {
                "network": "raw",
                "security": "none"
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        },
        {
            "listen": "0.0.0.0",
            "port": SS_PORT,
            "tag": "ss",
            "protocol": "shadowsocks",
            "settings": {
                "method": "2022-blake3-aes-256-gcm",
                "password": "SS_PASS"
            },
            "streamSettings": {
                "network": "raw",
                "security": "none"
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        }
    ],
    "routing": {
        "rules": [
            {
                "type": "field",
                "inboundTag": [
                    "sk5"
                ],
                "source": [
                    WHITELIST_IPS
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "inboundTag": [
                    "sk5"
                ],
                "outboundTag": "blocked"
            },
            {
                "type": "field",
                "inboundTag": [
                    "ss"
                ],
                "source": [
                    WHITELIST_IPS
                ],
                "outboundTag": "direct"
            },
            {
                "type": "field",
                "inboundTag": [
                    "ss"
                ],
                "outboundTag": "blocked"
            }
        ]
    }
}
