{
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": S5_PORT,
            "protocol": "socks",
            "settings": {
                "clients": [
                    {
                        "auth": "password",
                        "accounts": [
                            {
                                "user": "S5_USER",
                                "pass": "S5_PASS"
                            }
                        ],
                        "udp": true
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "raw",
                "security": "none"
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        },
        {
            "listen": "0.0.0.0",
            "port": SS_PORT,
            "protocol": "shadowsocks",
            "settings": {
                "clients": [
                    {
                        "settings": {
                            "method": "2022-blake3-aes-256-gcm",
                            "password": "SS_PASS"
                        }
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "raw",
                "security": "none"
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ]
            }
        }
    ]
}
